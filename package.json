{
  "name": "resumebuilder-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest watch",
    "validate:supabase": "tsx src/scripts/validateSupabase.ts",
    "validate:schema": "tsx src/scripts/validateSchema.ts",
    "migrate:up": "tsx src/scripts/runMigrations.ts up",
    "migrate:down": "tsx src/scripts/runMigrations.ts down",
    "migrate:status": "tsx src/scripts/runMigrations.ts status",
    "db:init": "tsx src/scripts/initializeDatabase.ts",
    "db:verify": "tsx src/scripts/verifyDatabase.ts",
    "test:supabase": "vitest src/tests/lib/supabase.test.ts",
    "test:schema": "vitest src/tests/database/schema-validation.test.ts",
    "test:migrations": "vitest src/tests/database/migration-system.test.ts",
    "test:health": "vitest src/tests/api/health-checks.test.ts",
    "test:backup": "vitest src/tests/database/backup-validation.test.ts",
    "backup:validate": "tsx src/scripts/validateBackup.ts",
    "backup:validate:json": "tsx src/scripts/validateBackup.ts --json",
    "health:database": "curl -f http://localhost:3000/api/health/database || echo 'Health check endpoint not available'",
    "health:services": "curl -f http://localhost:3000/api/health/services || echo 'Health check endpoint not available'",
    "health:overall": "curl -f http://localhost:3000/api/health/overall || echo 'Health check endpoint not available'",
    "validate:all": "npm run validate:supabase && npm run validate:schema && npm run backup:validate && npm run test:supabase && npm run test:schema && npm run test:migrations && npm run test:health && npm run test:backup",
    "bundle-analyze": "npx webpack-bundle-analyzer .next/static/chunks/*.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
